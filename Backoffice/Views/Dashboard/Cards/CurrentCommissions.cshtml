@model IEnumerable<RealTimeCommission>

@if (Model != null)
{
    if (Model.Count() > 0)
    {
        <h2 class="mb-0 hidden-value">
            $X,XXX
        </h2>
        <h2 class="mb-0 collapse real-value">
            @Format.Currency(Model.FirstOrDefault().Total, Model.FirstOrDefault().CurrencyCode)
        </h2>
        <div class="text-muted small">@Model.FirstOrDefault().Period.PeriodDescription</div>
    }
}
else
{
    <p>@Resources.Common.CardFailedToLoad</p>
}