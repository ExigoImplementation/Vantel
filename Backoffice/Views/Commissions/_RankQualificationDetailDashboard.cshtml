@model GetCustomerRankQualificationsResponse

@if (Model.QualificationLegs.Any())
{
    var leg = Model.QualificationLegs.ToArray()[0];

    <div class="row">
            <div class="col-12">

                <small class="text-uppercase">@Resources.Common.RankRequirements </small>
                <hr class="mt-1 border" />
                <div class="list-group list-group-flush">
                    @foreach (var requirement in leg.Requirements)
                    {
                        Html.RenderPartial("_RankRequirement", requirement);
                    }
                </div>
            </div>
    </div>
}
